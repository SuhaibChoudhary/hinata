<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/@fortawesome/fontawesome-free@6.2.1/css/fontawesome.min.css" integrity="sha384-QYIZto+st3yW+o8+5OHfT6S482Zsvz2WfOzpFSXMF9zqeLcFV0/wlZpMtyFcZALm" crossorigin="anonymous">
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/css/bootstrap.min.css" rel="stylesheet"
integrity="sha384-KK94CHFLLe+nY2dmCWGMq91rCGa5gtU4mk92HdvYe+M/SXH301p5ILy+dN9+nJOZ" crossorigin="anonymous">

<style>

  .sidenav {
    font-family: Monserrat, sans-serif;
    height: 100vh;
    width: 0;
    position: fixed;
    z-index: 1;
    top: 0;
    left: 0;
    background-color: rgb(35, 39, 42);
    overflow-x: hidden;
    transition: 0.5s;
    padding-top: 60px;
  }

  .sidenav a {
    font-family: Monserrat, sans-serif;
    padding: 8px 16px;
    text-decoration: none;
    font-size: 16px;
    color: #fff;
    display: block;
    transition: 0.3s;

  }

  .sidenav a:hover {
    background-color: #333;
  }

  .sidenav .closebtn {
    position: absolute;
    text-align: right;
    top: 0;
    right: 25px;
    font-size: 36px;
    margin-left: 50px;
    color: #fff;
  }

  .sidenav h6 {
    color: #fff;
    font-size: 11tpx;
    padding: 16px 8px;
    margin: 0;
 font-family: Monserrat, sans-serif;

  }

  @media screen and (max-height: 450px) {
    .sidenav {
      padding-top: 15px;
    }

    .sidenav a {
      font-size: 16px;
    }
  }

  .navbar {
    background-color: rgb(35, 39, 42);
    height: 80px;
    width: 100%;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    padding: 0 20px;
    box-sizing: border-box;
  }

  .navbar .openbtn {
  text-align: left;
  margin-right: auto; /* Add this line */
}


  .navbar img {
    width: 60px;
    height: 60px;
    border-radius: 50%;
    margin-right: 30px;
  }

  .navbar h2 {
    margin: 0;
    color: #fff;
    font-size: 18px;
    font-weight: normal;
  }

  .closebtn{
    width: 20px;
    height: 20px;
    margin:10px 10px;
    padding: auto;
  }
</style>
</style>

<div id="mySidenav" class="sidenav" >
    <img src="https://media.discordapp.net/attachments/1074644219844640801/1112271883459633182/close.png?width=427&height=427" href="javascript:void(0)" class="closebtn" onclick="closeNav()">
  <h6><strong>MAIN</strong></h6>
<a href="#"><span class="fa fa-home"> Home</span></a>
  <a href="#"><span class="fa fa-diamond"></span> Premium</a>
        
        <h6> <strong>ANNOUNCEMENTS</strong></h6>
  <a class="fa fa-bullhorn"href="#"> Announcement</a>
  <a href="/dashboard/<%= guild.id %>/announcement"><span class="fa fa-star"> Greet & Welcome</span></a>
 
</div>

<div class="nav navbar">
<img src="https://cdn.discordapp.com/attachments/1074644219844640801/1112271313046880316/menu.png" class="openbtn" onclick="openNav()">
<img src="<%= user.avatarURL %>"> <h2 style="font-size: 30px; padding-right: 10px;"><%= user.username %> <a href="/auth/logout" style="font-size: 30px; padding-right: 10px;" class="fa-solid fa-door-open"></a>
</div>
<script>
  function openNav() {
    if (window.innerWidth >= 768) {
      document.getElementById("mySidenav").style.width = "200px";
    } else {
      document.getElementById("mySidenav").style.width = "100%";
    }
  }

  function closeNav() {
    document.getElementById("mySidenav").style.width = "0";
  }
</script>
